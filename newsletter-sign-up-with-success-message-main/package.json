{
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@jest/globals": "^29.6.4",
    "babel-jest": "^29.6.4",
    "css-minify": "^2.0.0",
    "html-minifier-terser": "^7.2.0",
    "jest": "^29.6.4"
  },
  "scripts": {
    "build-html": "html-minifier-terser index.src.html --collapse-whitespace --remove-comments -o index.html",
    "build-css": "css-minify -d css -o css",
    "build": "npm run build-html & npm run build-css"
  }
}