{
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@jest/globals": "^29.6.4",
    "babel-jest": "^29.6.4",
    "css-minify": "^2.0.0",
    "html-minifier-terser": "^7.2.0",
    "http-server": "^14.1.1",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "build-html": "html-minifier-terser  --input-dir html --collapse-whitespace --remove-comments --output-dir .",
    "build-css": "css-minify -d css -o css",
    "build": "npm run build-html & npm run build-css",
    "serve": "npm run build && http-server .",
    "test": "jest"
  },
  "type": "module"
}